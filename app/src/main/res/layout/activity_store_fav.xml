<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:background="#FFF"
  tools:context=".activitys.store_fav.StoreFavActivity">
  <RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.viewpager.widget.ViewPager
      android:id="@+id/viewpage_store_fav"
      android:layout_width="match_parent"
      android:layout_height="match_parent" android:layout_alignParentStart="true"
      android:layout_alignParentTop="true" android:layout_marginTop="0dp" android:layout_marginStart="0dp"
      android:layout_marginBottom="0dp" android:layout_above="@+id/tab_bottom_outer"
      android:layout_alignParentLeft="true" android:layout_marginLeft="0dp">

    </androidx.viewpager.widget.ViewPager>


    <FrameLayout
      android:id="@+id/tab_bottom_outer"
      android:layout_width="match_parent"
      android:background="#F7F7F7"
      android:layout_height="150px" android:layout_marginBottom="0dp"
      android:layout_alignParentBottom="true">
      <com.google.android.material.tabs.TabLayout
        android:id="@+id/tab_stort_fav"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:tabTextColor="#181818"
        app:tabSelectedTextColor="#05C160"
        app:tabMode="fixed"
        app:tabIndicatorHeight="0dp"
        ></com.google.android.material.tabs.TabLayout>
    </FrameLayout>
  </RelativeLayout>
</FrameLayout>